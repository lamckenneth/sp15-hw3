<% @users.each do |user| %>
	<h1>User</h1>
	<h2><%= user.username %></h2>
	<h3><%= user.age %></h3>
	<h3><%= user.email %></h3>
<% end %>

<% @cats.each do |cat| %>
	<h1>Cat</h1>
	<h2><%= cat.name %></h2>
	<h3><%= cat.meow %></h3>
<% end %>

<% @todos.each do |todo| %>
	<h1>Todo</h1>
	<h3><%= todo.tasks %></h3>
	<h3><%= todo.finished %></h3>
<% end %>